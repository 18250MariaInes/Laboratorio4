        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Inicio">

    <ImageView
            android:layout_width="280dp"
            android:layout_height="255dp" app:srcCompat="@drawable/foto1"
            android:id="@+id/imageView" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="65dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="66dp"
            android:layout_marginBottom="84dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.481"/>
    <TextView
            android:text="María Inés Vásquez"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView2" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="144dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="144dp" android:layout_marginBottom="90dp"
            app:layout_constraintBottom_toTopOf="@+id/imageView"
            android:textSize="32sp" android:layout_marginTop="39dp" app:layout_constraintTop_toTopOf="parent"/>
</android.support.constraint.ConstraintLayout>