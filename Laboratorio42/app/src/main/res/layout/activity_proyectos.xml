<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Proyectos">


    <TextView
            android:text="Proyectos"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView3" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="163dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="165dp" android:layout_marginTop="32dp" app:layout_constraintTop_toTopOf="parent"
            android:textSize="32sp" app:layout_constraintHorizontal_bias="0.478"/>
    <Button
            android:text="Laboratorio 2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/buttonLab2" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="128dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="127dp"
            app:layout_constraintHorizontal_bias="0.0" android:layout_marginTop="141dp"
            app:layout_constraintTop_toBottomOf="@+id/textView3" android:onClick="abrirlaboratorio2"/>
    <Button
            android:text="Laboratorio 3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/buttonLab3" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="128dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="127dp" android:layout_marginTop="44dp"
            app:layout_constraintTop_toBottomOf="@+id/buttonLab2" android:onClick="abrirlaboratorio3"/>
    <WebView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/webView"
            android:layout_below="@+id/buttonLab3"
            android:layout_marginTop="29dp"
            app:layout_constraintTop_toBottomOf="@+id/buttonLab3" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="163dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="163dp"/>
</android.support.constraint.ConstraintLayout>